# only ac_max_sed_lines should be used.
: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='pure-ftpd'
PACKAGE_TARNAME='pure-ftpd'
PACKAGE_VERSION='1.0.21'
PACKAGE_STRING='pure-ftpd 1.0.21'
